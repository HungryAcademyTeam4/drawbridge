<div id="messages" class="span8">
  <div class="row">
    <div class="span1">
      <h3>From</h3>
    </div>
    <div id="message-head" class="span6">
      <h3>Message</h3>
    </div>
  </div>
  <% @messages.each do |message| %>
    <div class="row">
      <div class="span1">
        <p>User</p>
      </div>
      <div class="span6">
        <p><%= message %></p>
      </div>
    </div>
  <% end %>
</div>