<div id="messages" class="span8">
  <div class="row">
    <div class="span1">
      <h3>From</h3>
    </div>
    <div id="message-head" class="span6">
      <h3>Message</h3>
    </div>
  </div>
  <!-- JQTODO: This logic should be pushed into Chatbot -->
  <% if @messages %>
    <% @messages.each do |message| %>
      <%= render partial: "message", locals: { message: message } %>
    <% end %>
  <% else %>
    <div class="row">
      <div class="span1">
        <p></p>
      </div>
      <div class="span6">
        <p>There is no message</p>
      </div>
    </div>
  <% end %>
</div>